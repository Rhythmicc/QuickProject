#compdef detector

local cur prev
cur=${words[CURRENT]}
prev=${words[CURRENT-1]}

(( $+functions[_detector_main_zh] )) ||
_detector_main_zh() {
    local -a args
    args=(
        -ff:'对比两个文件',
        -pf:'对比程序运行结果和文件'
        -fp:'对比文件和程序运行结果'
        -pp:'对比两个程序的运行结果'
    )

    _describe -t common-commands 'common commands' args && _ret=0
}

if (( ${#words} < 3 )); then
    _detector_main_zh
fi
